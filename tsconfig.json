{
  "compilerOptions": {
    "module": "ESNext",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "target": "ESNext",
    "lib": ["DOM", "Dom.Iterable", "ESNext"],
    "strict": true,
    "moduleResolution": "bundler"
  },
  "files": [
    "src/components/Login.tsx",
    "src/App.tsx",
    "src/components/CitiesList.tsx",
    "src/main.tsx"
  ]
}
